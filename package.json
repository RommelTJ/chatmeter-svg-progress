{
  "name": "chatmeter-svg-progress",
  "version": "0.0.1",
  "description": "Chatmeter SVG Progress UI Component for React",
  "keywords": [
    "react",
    "react-component",
    "react-progress",
    "progress",
    "chatmeter",
    "svg"
  ],
  "homepage": "https://github.com/RommelTJ/chatmeter-svg-progress",
  "repository": {
    "type": "git",
    "url": "git@github.com:RommelTJ/chatmeter-svg-progress.git"
  },
  "bugs": {
    "url": "https://github.com/RommelTJ/chatmeter-svg-progress/issues"
  },
  "scripts": {
    "start": "dumi dev",
    "docs:build": "dumi build",
    "docs:deploy": "gh-pages -d docs-dist",
    "build": "father-build",
    "deploy": "npm run docs:build && npm run docs:deploy",
    "release": "npm run build && npm publish",
    "prettier": "prettier --write \"**/*.{js,jsx,tsx,ts,less,md,json}\"",
    "test": "umi-test",
    "test:coverage": "umi-test --coverage"
  },
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "typings": "dist/index.d.ts",
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,less,md,json}": [
      "prettier --write"
    ],
    "*.ts?(x)": [
      "prettier --parser=typescript --write"
    ]
  },
  "dependencies": {
    "classnames": "^2.2.6",
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "react-test-renderer": "^16.14.0"
  },
  "devDependencies": {
    "@jest/globals": "^26.6.2",
    "@types/classnames": "^2.2.11",
    "@types/react": "^16.14.0",
    "@types/react-dom": "^16.9.11",
    "@umijs/test": "^3.0.5",
    "dumi": "^1.0.13",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.6",
    "enzyme-to-json": "^3.6.1",
    "father-build": "^1.17.2",
    "gh-pages": "^3.0.0",
    "lint-staged": "^10.0.7",
    "prettier": "^1.19.1",
    "yorkie": "^2.0.0"
  }
}
